pipeline {
    agent any
    stages {
            stage('Clear Docker Components') {
            steps {
                script {
                
                    def count = sh(script: "docker ps | grep imagename | wc -l", returnStdout: true).trim()
                    def num = "$count" as Integer
                    if (num.compareTo(0)){
                        echo count
                        echo "big"
                    }
                }
            }
        }
    }
}
